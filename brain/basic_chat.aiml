    <topic name="MOREINVESTIGATION"> -->
    <category>

        <pattern>NO</pattern>
        <that>* WANT TO CONTINUE</that>
        <template>
            <think>
                <set name="topic">default</set>
            </think>
            Bad Choice
        </template>
    </category>
    <!-- </topic> -->
    <set name="topic">default</set>
    <category>
        <pattern>MY NAME IS *</pattern>
        <template>
            <think>
                <set name="name">
                    <star index="1" />
                </set>
            </think>
            Hey there
            <get name="name"></get>
            ,how may i help you
        </template>
    </category>


    <category>
        <pattern>MY NAME</pattern>
        <template>
            <think>
                <set name="topic">default</set>
                <set name="name">
                    <star index="1" />
                </set>
            </think>
            Hey there
            <get name="name"></get>
            ,how may i help you
        </template>
    </category>


    <category>
        <pattern>WHAT * NAME</pattern>
        <template>
            <random>
                <li>
                    As far as i remembered, your name is
                    <get name="name"></get>
                </li>
                <li>
                    <get name="name"></get>
                    is your name
                </li>
            </random>
        </template>

    </category>


    <category>
        <pattern>HEY THERE</pattern>
        <template>
            <think>
                <set name="topic">query</set>
            </think>
            <random>
                <li>I am Medicare Are you sick!</li>
                <li>My name is Medicare Are you sick!</li>
                <li>People call me Medicare Are you sick?</li>
                <li>I am Medicare Are you sick?</li>
            </random>
        </template>
    </category>


