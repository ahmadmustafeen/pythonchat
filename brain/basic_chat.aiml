    <!-- SYPMTOMS MEASURE-->
    <!-- <topic name="SYMPTOMS"> -->
    <category>
        <that>* SYMPTOMS * FEELING *</that>
        <pattern>I FEEL *</pattern>
        <template>
            <think>
                <set name="topic">severity</set>
                <set name="symptom">
                    <star />
                </set>
            </think>
            so you're having symptoms
            <get name="symptom" />
            , thats bad
                How bad in scale of 1-10
        </template>
    </category>
